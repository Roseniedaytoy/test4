#include<stdio.h>
#include<string.h>
int main()
{
	char strExp[]="2*2/4*1/1*2*3/2";
	int res=strExp[0]-'0';
	int a=strlen(strExp);
	for(int i=1;i<a;i++)
	{
		if(strExp[i]=='*')
		{
			int rightoperand=strExp[i+1]-'0';
			res=res*rightoperand;
			i++;
		}
		else if(strExp[i]=='/')
		{
			int leftoperand=strExp[i+1]-'0';
			res=res/leftoperand;
			i++;
		}
	}
	printf("res=%d\n",res);
	return 0;
}
